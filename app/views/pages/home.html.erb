          <%= link_to "Vietnam", "#", class: "btn btn-info btn-xs", style: "border-radius: 20px"  %>
          <%= link_to "Jamaica", "#", class: "btn btn-warning btn-xs", style: "border-radius: 20px" %>
          <%= link_to "Laos", "#", class: "btn btn-danger btn-xs", style: "border-radius: 20px"%>
          <%= link_to "Portugal", "#", class: "btn btn-success btn-xs", style: "border-radius: 20px" %>
          <%= link_to "Thailand", "#", class: "btn btn-xs", style: "border-radius: 20px; color:black;" %>
          <%= link_to "Vietnam", "#", class: "btn btn-primary btn-xs", style: "border-radius: 20px"%>
